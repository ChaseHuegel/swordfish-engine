#version 1.0;
#namespace WaywardBeyond.Client.Core.Saves;

message Version
{
    uint DataVersion   = 0;
    string Name        = 1;
    string Environment = 2;
}

message Level
{
    Version Version = 0;
    
    int Seed = 1;
    
    long LastPlayedMs = 2;
    long AgeMs        = 3;
    
    int SpawnX = 4;
    int SpawnY = 5;
    int SpawnZ = 6;
}