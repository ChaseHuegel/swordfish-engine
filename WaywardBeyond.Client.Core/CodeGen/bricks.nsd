#version 1.0;
#namespace WaywardBeyond.Client.Core.Serialization;

message RawBrick
{
    int X;
    int Y;
    int Z;
    ushort ID;
    byte Data;
    byte Orientation;
}

message RawBrickGrid
{
    RawBrick[] Bricks;
    float? X;
    float? Y;
    float? Z;
    float? OrientationX;
    float? OrientationY;
    float? OrientationZ;
    float? OrientationW;
}